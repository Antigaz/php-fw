{
  "routes": {
    "candidates": {
      "path": "/students",
      "methods": [
        "GET"
      ],
      "action": "Metinet:Controllers:CandidatesController::retrieveMemberList"
    },
    "event": {
      "path": "/events",
      "methods": [
        "GET"
      ],
      "action": "Metinet:Controllers:EventController::retrieveEventList"
    },
    "hello": {
      "path": "/hello",
      "methods": [
        "GET"
      ],
      "action": "Metinet:Controllers:CandidatesController::sayHello"
    },
    "bonjour": {
      "path": "/bonjour",
      "methods": [
        "GET"
      ],
      "action": "Metinet:Controllers:CandidatesController::disBonjour"
    },
    "security_signup": {
      "path": "/signup",
      "methods": ["GET"],
      "action": "Metinet:Controllers:SecurityController::signUp"
    }
  }
